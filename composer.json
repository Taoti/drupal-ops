{
    "name": "taoti/drupal-ops",
    "description": "Taoti ops files.",
    "type": "project",
    "license": "MIT",
    "conflict": {
        "drupal/core": "<8.8"
    },
    "require": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "drupal/core-dev": "^8.8 | ^9",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9.0",
        "slevomat/coding-standard": "^5.0|^6.0"
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.lando.base.yml": "assets/.lando.base.yml",
                "[project-root]/default.lando.local.yml": "assets/default.lando.local.yml",
                "[project-root]/phpcs.xml.dist": "assets/phpcs.xml.dist"
            }
        }
    }
}
